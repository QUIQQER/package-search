<?xml version="1.0" encoding="UTF-8"?>
<database>

    <projects>
        <table name="searchQuick" no-auto-update="1" no-site-reference="1">
            <field type="bigint(11)">id</field>
            <field type="TEXT">urlParameter</field>
            <field type="TEXT">data</field>
            <field type="TEXT">rights</field>
        </table>

        <table name="searchFull" no-auto-update="1" no-site-reference="1">
            <field type="bigint(11)">id</field>
            <field type="TEXT">urlParameter</field>
            <field type="VARCHAR(256)">name</field>
            <field type="VARCHAR(256)">title</field>
            <field type="TEXT">short</field>
            <field type="TEXT">data</field>
            <field type="TEXT">short</field>
            <field type="VARCHAR(128)">datatype</field>

            <primary>id</primary>
            <fulltext>data</fulltext>
        </table>
    </projects>

</database>
